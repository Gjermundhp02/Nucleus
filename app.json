{
  "expo": {
    "owner": "loginapp",
    "name": "Login",
    "slug": "Login",
    "version": "1.1.10",
    "orientation": "portrait",
    "icon": "./assets/logo/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/952a1914-0c53-43e7-b64e-8daab0b3a435"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/loginText.png",
          "color": "#000000",
          "sounds": [
            "./assets/sounds/notificationlogin.wav",
            "./assets/sounds/notificationsound.wav"
          ]
        }
      ],
      ["@react-native-firebase/app"],
      [
        "expo-calendar",
        {
          "calendarPermission": "Login wants to save events to your calendar."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eirikhanasand.Login",
      "buildNumber": "43",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
         "UIBackgroundModes": [
           "fetch",
           "remote-notification"
         ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/adaptiveIcon.png",
        "backgroundColor": "#000000"
      },
      "notification": {
        "icon": "./assets/logo/adaptiveIcon.png",
        "androidMode": "default",
        "priority": "max",
        "visibility": "public",
        "smallIconSize": 24
      },
      "package": "com.login.Login",
      "googleServicesFile": "./google-services.json",
      "versionCode": 34,
      "permissions": [
        "NOTIFICATIONS",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/logo/favicon.ico"
    },
    "extra": {
      "eas": {
        "projectId": "952a1914-0c53-43e7-b64e-8daab0b3a435"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
